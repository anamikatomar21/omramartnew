{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    base
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/onboarding/dashboard/Upload_Product", "components/Category", "components/ProductCard", "components/SmallBanner"],
  "exclude": ["node_modules"]
}
